# Definition for singly-linked list.
# class ListNode:
#     def _init_(self, val=0, next=None):
#         self.val = val
#         self.next = next
class Solution:
    def swapNodes(self,head,k):

        dummy = ListNode()
        dummy.next = head
        res = dummy

        for i in range(k):
            dummy= dummy.next
        first =  dummy
        second = res

        while dummy:
            second = second.next
            dummy = dummy.next
        first.val,second.val = second.val,first.val
        return res.next
